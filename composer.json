    "lullabot-amp": {
      "type": "vcs",
      "url": "git@github.com:dennisinteractive/amp-library.git",
      "no-api": true
    },
    "masonry": {
      "type": "package",
      "package": {
        "name": "lib/masonry",
        "version": "3.2.2",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/desandro/masonry",
          "type": "git",
          "reference": "v3.2.2"
        }
      }
    },
    "ckeditor": {
      "type": "package",
      "package": {
        "name": "lib/ckeditor",
        "version": "4.6.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.cksource.com/CKEditor/CKEditor/CKEditor%204.6.1/ckeditor_4.6.1_full.zip",
          "type": "zip",
          "reference": "master"
        }
      }
    },
    "photoswipe": {
      "type": "package",
      "package": {
        "name": "lib/photoswipe",
        "version": "3.0.5",
        "type": "drupal-library",
