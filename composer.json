						    "name": "lib/superfish",
						    "version": "1.0",
						    "type": "drupal-library",
						    "source": {
								    "url": "https://github.com/mehrpadin/Superfish-for-Drupal",
								    "type": "git",
								    "reference": "master"
						    }
				    }
		    }
    },
