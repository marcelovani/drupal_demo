  "scripts":{
     "post-install-cmd":[
        "chmod 0755 ./web/core; cd ./web/core; rm -rf sites; ln -s ../sites;"
     ],
     "post-update-cmd":[
        "chmod 0755 ./web/core; cd ./web/core; rm -rf sites; ln -s ../sites;"
     ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "drupal/drupal"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/sites/default/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/sites/default/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/sites/default/themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "drupal-scaffold": {
      "excludes": [
        "web.config"
      ]
    }
  },
  "require-dev": {
    "drupal/xhprof": "^1.0@beta"
  }
