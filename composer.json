{
    "name": "marcelovani/drupal_demo",
    "description": "Drupal demo site.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,

    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],

    "scripts": {
        "post-install-cmd": [
            "@composer drupal-scaffold"
        ],
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },

    "extra": {
        "installer-paths": {
          "web/core": ["drupal/core"],
          "web/profiles/contrib/{$name}": ["type:drupal-profile"],
          "web/modules/contrib/{$name}": ["type:drupal-module"],
          "web/themes/contrib/{$name}": ["type:drupal-theme"],
          "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "patches": {
        }
    },

    "require-dev": {
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "drush/drush": "^9.0",
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "2.*",
        "drupal/devel": "^1.0"
    },

    "require": {
        "cweagans/composer-patches": "^1.5.0",
        "composer/installers": "^1.0",
        "drupal/core": "^8.1.0",
        "drupal/config_update": "^1.1",
        "drupal/captcha_keypad": "^1.0",
        "drupal/sharerich": "^1.0",
        "drupal/boxout": "^1.0",
        "drupal/colorbox": "1.x-dev",
        "drupal/metatag": "1.x-dev",
        "drupal/global_redirect": "1.x-dev",
        "drupal/menu_block": "1.x-dev",
        "drupal/redirect": "1.x-dev",
        "drupal/superfish": "1.x-dev",
        "drupal/context": "1.x-dev",
        "drupal/mollom": "1.x-dev",
        "drupal/calendar": "1.x-dev",
        "drupal/media": "1.x-dev",
        "drupal/advanced_help": "1.x-dev",
        "drupal/devel": "1.x-dev",
        "drupal/login_tobogan": "1.x-dev",
        "drupal/token": "1.x-dev",
        "drupal/content_access": "1.x-dev",
