          "url": "http://github.com/downloads/codecomputerlove/PhotoSwipe/code.photoswipe-3.0.5.zip",
          "type": "zip",
          "reference": "master"
        }
      }
    },
    "plupload": {
      "type": "package",
      "package": {
        "name": "lib/plupload",
        "version": "1.5.4",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/downloads/moxiecode/plupload/plupload_1_5_4.zip",
          "type": "zip",
          "reference": "master"
        }
      }
    },
    "jquery.cycle": {
      "type": "package",
      "package": {
        "name": "lib/jquery.cycle",
        "version": "3.0.3",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/malsup/cycle.git",
          "type": "git",
          "reference": "3.0.3"
        }
      }
    },
    "json2": {
      "type": "package",
      "package": {
        "name": "lib/json2",
        "version": "master",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/douglascrockford/JSON-js",
          "type": "git",
          "reference": "master"
        }
      }
    },
    "rrssb": {
      "type": "package",
      "package": {
        "name": "lib/rrssb",
        "version": "1.6.0",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/kni-labs/rrssb",
          "type": "git",
          "reference": "v1.6.0"
        }
      }
    },
    "superfish": {
      "type": "package",
      "package": {
        "name": "lib/superfish",
        "version": "dev-1.x",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/mehrpadin/Superfish-for-Drupal",
          "type": "git",
          "reference": "1.x"
        }
      }
    },
    "flexslider": {
      "type": "package",
      "package": {
        "name": "lib/flexslider",
        "version": "2.6.3",
        "type": "drupal-library",
        "source": {
          "url": "https://github.com/woocommerce/FlexSlider",
          "type": "git",
          "reference": "master"
        }
      }
    }
