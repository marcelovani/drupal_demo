{
    "name": "marcelovani/alophyshiotherapy",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "pdo": {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        },
		    "drupal": {
				    "type": "package",
				    "package": {
						    "name": "drupal/core",
						    "version": "7",
						    "type": "drupal-core",
						    "source": {
								    "url": "https://git.drupal.org/project/drupal.git",
								    "type": "git",
								    "reference": "7.x"
						    }
				    }
		    },
		    "masonry": {
				    "type": "package",
				    "package": {
						    "name": "lib/masonry",
						    "version": "3.2.2",
						    "type": "drupal-library",
						    "source": {
								    "url": "https://github.com/desandro/masonry",
								    "type": "git",
								    "reference": "master"
						    }
				    }
		    },
		    "ckeditor": {
				    "type": "package",
				    "package": {
						    "name": "lib/ckeditor",
						    "version": "4.6.2",
